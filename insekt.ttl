@prefix insekt: <https://example.org/insekten/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

insekt: a skos:ConceptScheme ;
  dct:title "Insekten Europas"@de, "Insects in Europe"@en ;
  dct:creator "Katharina Niederprüm"@de ;
  dct:created "2022-02-21" ;
  dct:license <https://creativecommons.org/publicdomain/zero/4.0/> ;
  dct:description "Auszug aus der Systematik der Insekten Europas"@de, "Some common insects in Europe"@en ;
  skos:hasTopConcept insekt:Borstenschwaenze, insekt:Springschrecken, insekt:Schnabelkerfe, insekt:Schmetterling .
  
insekt:Borstenschwaenze a skos:Concept ;
  skos:prefLabel "Borstenschwänze"@de, "Thysanura"@en ;
  skos:altLabel "Zottenschwänze"@de, "Thysanura"@de, "Apterygota"@en;
  skos:note "Primitive, flügellose Art aus der Gruppe der Flügellosen ohne eigentliche Metamorphose."@de, "Thysanura are wingless and have bodies covered with fine scales"@en ;
  skos:historyNote "Früher Ordnung der Urinsekten. Heute in zwei Ordnungen aufgeteilt: Silberfischen und Felsenspringer"@de, "Thysanura is the now deprecated name of what was, for over a century, recognised as an order in the class Insecta. The two constituent groups within the former order are the Archaeognatha (jumping bristletails) and the Zygentoma (silverfish and firebrats)."@en ;
  skos:topConceptOf insekt: .

insekt:Springschrecken a skos:Concept ;
  skos:prefLabel "Springschrecken"@de,  "Orthoptera"@en ;
  skos:altLabel "Heuschrecken"@de, "Saltatoria"@lat ; 
  skos:hiddenLabel "Grashüpfer"@de, "grasshopper"@en ;
  skos:note "In dieser Ordnung sind etwa 17000 Arten bekannt, von denen mehr als die Hälfte Heuschrecken sind."@de, "Orthoptera is an order of insects that comprises the grasshoppers, locusts, and crickets, including closely related insects, such as the bush crickets or katydids and wētā. The order is subdivided into two suborders: Caelifera – grasshoppers, locusts, and close relatives; and Ensifera – crickets and close relatives." @en ;
  skos:topConceptOf insekt: .

insekt:Schnabelkerfe a skos:Concept ;
  skos:prefLabel "Schnabelkerfe"@de, "Hemiptera"@lat, "Hemiptera"@en ;
  skos:altLabel "Rhynchota"@de, "Rhynchota"@lat, "true bugs"@en ;
  skos:definition "Die Schnabelkerfe (Hemiptera, Rhynchota) sind eine Insekten-Ordnung innerhalb der Neuflügler (Neoptera)."@de, "Hemiptera is the largest order of hemimetabolous insects."@en;
  skos:narrower insekt:Wanzen, insekt:Gleichfluegler ;
  skos:topConceptOf insekt: .

  insekt:Wanzen a skos:Concept ;
    skos:prefLabel "Wanzen"@de, "Heteroptera"@lat, "Heteroptera"@en ;
    skos:altLabel "true bugs"@en, "typical bugs"@en ;
    skos:definition "Schnabelkerfe, deren Vorderflügel aus einem ledrigen Grundabschnitt und einem häutigen Spitzenteil bestehen"@de ;
    skos:broader insekt:Schnabelkerfe ;
    skos:inScheme insekt: .

  insekt:Gleichfluegler a skos:Concept ;
    skos:prefLabel "Gleichflügler"@de, "Homoptera"@lat ;
    skos:definition "Schnabelkerfen, bei denen die Vorderflügel, sofern überhaupt ausgebildet, einheitlich ledrig oder häutig sind."@de ;
    skos:broader insekt:Schnabelkerfe ;
    skos:narrower insekt:Zikaden, insekt:Blattlaeuse;
    skos:inScheme insekt: .

    insekt:Zikaden a skos:Concept ;
      skos:prefLabel "Zikaden"@de, "Auchenorrhyncha"@lat ;
      skos:broader insekt:Gleichfluegler ;
      skos:inScheme insekt: .

    insekt:Blattlaeuse a skos:Concept ;
     skos:prefLabel "Blattläuse"@de, "Aphidoidea"@lat ;
     skos:definition "sehr kleine, tropfenförmige, Pflanzensäfte saugende Schnabelkerfe"@de ;
     skos:broader insekt:Gleichfluegler ;
     skos:narrower insekt:Reblaus, insekt:Gruene-Fichtengallenlaus ;
     skos:inScheme insekt: .

      insekt:Reblaus a skos:Concept ;
      skos:prefLabel "Reblaus"@de, "Viteus vitifolii"@lat ;
      skos:note "Amerikanischer Rebenschädling, in Europa weit verbreitet"@de ;
      skos:broader insekt:Blattlaeuse ;
      skos:inScheme insekt: .

      insekt:Gruene-Fichtengallenlaus a skos:Concept ;
       skos:prefLabel "Grüne Fichtengallenlaus"@de, "Adelges viridis"@lat ;
       skos:broader insekt:Blattlaeuse ;
       skos:note "Erzeugt zapfenartige Gallen an Fichtentrieben"@de ;
       skos:inScheme insekt: .

insekt:Schmetterling a skos:Concept ;
  skos:prefLabel "Schmetterlinge"@de, "Lepidoptera"@lat ;
  skos:note "Eine sehr große Ordnung mit etwa 150000 bekannten Arten. Etwa 5000 Arten kommen in Europa vor, die Hälfte davon auch auf den Britischen Inseln."@de ;
  skos:narrower insekt:Schwalbenschwanz, insekt:Weissling ;
  skos:topConceptOf insekt: .

  insekt:Schwalbenschwanz a skos:Concept ;
    skos:prefLabel "Schwalbenschwanz"@de, "Papilinodae"@lat ;
    skos:altLabel "Apollofalter"@de, "Papilinodae"@lat ;
    skos:note "Meist große, schön gefärbte Schmetterlinge"@de ;
    skos:broader insekt:Schmetterling ;
    skos:inScheme insekt: .

  insekt:Weissling a skos:Concept ;
    skos:prefLabel "Weisslinge"@de, "Pieridae"@lat ;
    skos:note "Eine große Familie, meist weiß oder gelb, häufig schwarz gezeichnet. Geschlechter oft unterschiedlich gefärbt."@de ;
    skos:broader insekt:Schmetterling ;
    skos:narrower insekt:Kleiner-Kohlweissling, insekt:Rapsweissling ;
    skos:inScheme insekt: .

    insekt:Kleiner-Kohlweissling a skos:Concept ;
      skos:prefLabel "Kleiner Kohlweißling"@de, "Pieris rapae"@lat ;
      skos:note "Überall auf Kohlarten, wilden Kreuzblütlern, häufig recht schädlich"@de ;
      skos:broader insekt:Weissling ;
      skos:inScheme insekt: .

    insekt:Rapsweissling a skos:Concept ;
      skos:prefLabel "Rapsweißling"@de, "Pieris napi"@lat ;
      skos:broader insekt:Weissling ;
      skos:inScheme insekt: .
